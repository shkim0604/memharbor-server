AGORA_APP_ID=REPLACE_ME
AGORA_APP_CERT=REPLACE_ME
AGORA_CUSTOMER_ID=REPLACE_ME
AGORA_CUSTOMER_SECRET=REPLACE_ME
AGORA_CLOUD_RECORDING_BASE_URL=https://api.sd-rtn.com
MEMHARBOR_SECRET_KEY=REPLACE_ME
MEMHARBOR_DEBUG=0

# ============================================
# Push Notification Configuration
# ============================================

# iOS APNs VoIP Push Configuration
# Get these from Apple Developer Portal
APNS_TEAM_ID=REPLACE_ME
APNS_KEY_ID=REPLACE_ME
APNS_BUNDLE_ID=com.yourcompany.yourapp
APNS_USE_SANDBOX=1
# Path to .p8 key file (AuthKey_XXXXX.p8)
APNS_KEY_PATH=/path/to/AuthKey.p8
# OR provide key content directly (with escaped newlines)
# APNS_KEY_CONTENT=-----BEGIN PRIVATE KEY-----\nMIGT....\n-----END PRIVATE KEY-----

# Android FCM Configuration
# Get these from Firebase Console
FCM_PROJECT_ID=your-firebase-project-id
# Path to service account JSON file
FCM_SERVICE_ACCOUNT_PATH=/path/to/service-account.json
# OR provide JSON content directly
# FCM_SERVICE_ACCOUNT_JSON={"type":"service_account",...}
